{
  "defaultProfile":"fast",
  "clientPolicies":{
    "bailian_default":{
      "openaiCompat":{
        "enableThinking":true,
        "webSearch":{
          "enabled":false,
          "onlyWhenNoFunctionTools":false
        }
      }
    },
    "bailian_search":{
      "openaiCompat":{
        "enableThinking":true,
        "webSearch":{
