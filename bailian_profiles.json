      "provider":{
        "bailian":{
          "thinkingBudget":256,
          "forcedSearch":true,
          "searchStrategy":"max"
        }
      }
    }
  },
  "profiles":{
    "fast":{"model":"qwen3.5-plus","clientPolicy":"bailian_search"},
    "writer":{"model":"qwen3.5-plus","clientPolicy":"bailian_search"},
    "reasoning":{"model":"qwen3.5-plus","clientPolicy":"bailian_search"},
    "research_web":{"model":"qwen3.5-plus","clientPolicy":"bailian_search"}
  }
}